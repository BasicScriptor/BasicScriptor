local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()

local PhantomForcesWindow = Library:NewWindow("[üçïPIZZA] TMNT Battle Tycoon")

local KillingCheatsObby = PhantomForcesWindow:NewSection("Item")

KillingCheatsObby:CreateButton("UGC", function()
    local args = {
        [1] = "18443351454"
    }
    game:GetService("ReplicatedStorage").Packages._Index:FindFirstChild("sleitnick_knit@1.7.0").knit.Services.EventService.RF.ClaimUGC:InvokeServer(unpack(args))
end)


local KillingCheatsYT = PhantomForcesWindow:NewSection("YT:BasicScriptor")

KillingCheatsYT:CreateButton("Copy YouTube Channel Link", function()
    setclipboard("https://www.youtube.com/@BasicScriptor")
end)
