local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()

local PhantomForcesWindow = Library:NewWindow("ðŸ”¥Eat Slimes to Grow HUGEðŸ”¥")

local autoClickEnabled = false

local KillingCheats = PhantomForcesWindow:NewSection("Power Slime")

KillingCheats:CreateTextbox("put size of number", function(text)
    local args = {
    [1] = text
}

game:GetService("ReplicatedStorage"):WaitForChild("Honeypot"):WaitForChild("Internal"):WaitForChild("RemoteStorage"):WaitForChild("AwardSpinSize - RemoteEvent"):FireServer(unpack(args))
end)

KillingCheats:CreateButton("Reset", function()
game:GetService("ReplicatedStorage"):WaitForChild("Honeypot"):WaitForChild("Internal"):WaitForChild("RemoteStorage"):WaitForChild("SpawnPlayer - RemoteEvent"):FireServer()

end)

local KillingCheats = PhantomForcesWindow:NewSection("Skin")

KillingCheats:CreateTextbox("Put any number 1 to 46", function(text)
    local args = {
        [1] = tonumber(text)
    }

    game:GetService("ReplicatedStorage"):WaitForChild("Honeypot"):WaitForChild("Internal"):WaitForChild("RemoteStorage"):WaitForChild("EquipSkin - RemoteEvent"):FireServer(unpack(args))
end)

local KillingCheats = PhantomForcesWindow:NewSection("Player")

KillingCheats:CreateButton("Infinite yield", function()
loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()

end)

local KillingCheats = PhantomForcesWindow:NewSection("BasicScriptor")

KillingCheats:CreateButton("Copy YouTube Channel Link", function()
    setclipboard("https://www.youtube.com/@BasicScriptor")
end)
